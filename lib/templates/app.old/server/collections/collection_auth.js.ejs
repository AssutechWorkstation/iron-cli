<% if(realExtension === '.coffee') { %>
#
# Add allow rules like this:
# WARNING: Uncommenting the following code will ALLOW ANYTHING!
#  <%= name %>.allow
#    insert: (userId, doc) ->
#      true
#
#    update: (userId, doc, fieldNames, modifier) ->
#      true
#
#    remove: (userId, doc) ->
#      true
#

#
# Add deny rules like this:
# WARNING: Uncommenting the following code will DENY NOTHING!
#  <%= name %>.deny
#    insert: (userId, doc) ->
#      false
#
#    update: (userId, doc, fieldNames, modifier) ->
#      false
#
#    remove: (userId, doc) ->
#      false
#
<% } else { %>
/*
 * Add allow rules like this:
 * WARNING: Uncommenting the following code will ALLOW ANYTHING!
 *
 *  <%= name %>.allow({
 *    insert: function (userId, doc) {
 *      return true;
 *    },
 *
 *    update: function (userId, doc, fieldNames, modifier) {
 *      return true;
 *    },
 *
 *    remove: function (userId, doc) {
 *      return true;
 *    }
 *  });
 */

/*
 * Add deny rules like this:
 * WARNING: Uncommenting the following code will DENY NOTHING!
 *
 *  <%= name %>.deny({
 *    insert: function (userId, doc) {
 *      return false;
 *    },
 *
 *    update: function (userId, doc, fieldNames, modifier) {
 *      return false;
 *    },
 *
 *    remove: function (userId, doc) {
 *      return false;
 *    }
 *  });
 */
<% } %>
