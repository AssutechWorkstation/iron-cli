<% if(realExtension === '.coffee') { %>
#
# <%= name %> Publish Functions
#
Meteor.publish "<%= filename %>", ->
  # you can remove this if you return a cursor
  @ready()
  return
<% } else { %>
/*****************************************************************************/
/* <%= name %> Publish Functions
/*****************************************************************************/

Meteor.publish('<%= filename %>', function () {
  // you can remove this if you return a cursor
  this.ready();
});
<% } %>
